{
  "name": "bt-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "prettier:js": "prettier --check \"**/*.{ts,js,vue}\" --ignore-path .gitignore",
    "prettier": "yarn prettier:js",
    "prettier:fix": "yarn prettier:js --write",
    "lint:js": "eslint --max-warnings=0 --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lint:fix": "yarn lint:js --fix",
    "prepare": "husky install",
    "test": "jest --no-coverage",
    "test:coverge": "jest",
    "format": "yarn lint:fix && yarn prettier:fix"
  },
