    "@nuxtjs/i18n": "^7.3.1",
    "@nuxtjs/proxy": "^2.1.0",
    "@types/js-cookie": "^3.0.3",
    "core-js": "^3.25.3",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "vue": "^2.7.10",
    "vue-password-strength-meter": "^1.7.2",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "zxcvbn": "^4.4.2"
